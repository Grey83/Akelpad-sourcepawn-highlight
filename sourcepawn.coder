$~csgo-show_survival_respawn_status
stock void ShowClientMessage(int client, const char[] text, any ...)
{
	Event msg = CreateEvent("show_survival_respawn_status", true);
	if(!msg)
		return;

	int len = strlen(text) + 255;
	char[] formatted = new char[len];
	VFormat(formatted, len, text, 3);
	ReplaceString(formatted, len, "<", "<span color='#FFFF00'>");
	ReplaceString(formatted, len, ">", "</span>");
	Format(formatted, len, "<span class='fontSize-xxl mono-spaced-font-bold'>%s</span>", formatted);

	msg.SetString("loc_token", formatted);
	msg.SetInt("duration", 1);
	msg.SetInt("userid", -1);
	if(client) msg.FireToClient(client);
	else for(int i = 1; i <= MaxClients; i++) if(IsClientInGame(i) && !IsFakeClient(i)) msg.FireToClient(i);
	msg.Cancel();
}

$~csgo-point_worldtext
stock int Point_WorldText(float fPos[3], float fAngles[3], char[] sText = "Source 2 Engine?", iSize = 10, int r = 0xffffff, any ...)
{
	int ent = CreateEntityByName("point_worldtext");
	if(ent == -1)
		return -1;

	char buffer[512];
	VFormat(buffer, sizeof(buffer), sText, 8);
	DispatchKeyValue(ent, "message", buffer);

	IntToString(iSize, buffer, 4);
	DispatchKeyValue(ent, "textsize", buffer);

	FormatEx(buffer, 12, "%d %d %d", r, g, b);
	DispatchKeyValue(ent, "color", buffer);

	TeleportEntity(ent, fPos, fAngles, NULL_VECTOR);

	return ent;
}

