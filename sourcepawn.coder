$~cheat_cmd
stock void ExecuteCheatCommand(int client, const char[] cmd, const char[] arg = "")
{
	if(!client || !IsClientInGame(client) || !cmd[0])
		return;

	int flags = GetCommandFlags(cmd);
	SetCommandFlags(cmd, flags & ~FCVAR_CHEAT);
	if(!arg[0]) FakeClientCommand(client, cmd);
	else FakeClientCommand(client, "%s \"%s\"", cmd, arg);
	SetCommandFlags(cmd, flags);
}
